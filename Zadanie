enum Engine: String {
    case on =  "Заведен"
    case off = "Заглушен"
    mutating func button() {switch self {
        case .off:
        self = .on
        case .on:
        self = .off }

    }
}
enum Wind: String {
    case open =  "Открыто"
    case closed = "Закрыто"
      mutating func richag() {
        switch self {
        case .open:
        self = .closed
        case .closed:
        self = .open }
}
}
enum Trunk {
    case pust(empty: String)
    case polon(full: String)
    case partly(kg: Float)
}
protocol Vehicle {
var brand: String {get}
var year: Int {get}
var trunk: Trunk {get set}
var engine: Engine{get set}
var windows: Wind {get set}
}
